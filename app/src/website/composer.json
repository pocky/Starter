{
    "name": "black-project/website",
    "license": "MIT",
    "type": "project",
    "description": "The \"Black Edition\" distribution build on top of \"Symfony Standard Edition\"",
    "autoload": {
        "psr-4": {
            "Black\\Website\\": "src/",
            "Black\\Bundle\\WebsiteBundle\\": "src/Infrastructure/Bridge/Symfony/WebsiteBundle/",
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "app/tests/" }
    },
    "require": {
        "php": ">=7.0",
        "ramsey/uuid": "^2.8",
        "black/common": "dev-master",
    },
    "require-dev": {
        "behat/behat": "~3.0",
        "phpspec/phpspec": "^2.4",
        "phpspec/prophecy": "dev-master",
        "memio/spec-gen": "^0.4",
        "rmiller/behat-spec": "^0.3.0",
        "mikey179/vfsStream": "^1.6",
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/black-project/website/issues"
    }
}
